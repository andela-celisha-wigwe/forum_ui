{
  "name": "roy_forums_ui",
  "version": "1.0.0",
  "description": "just a ui for the rails project",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js --plugins transform-react-jsx src -w --out-dir build",
    "test": "./node_modules/mocha/bin/mocha --require babel-core/register -R spec",
    "start": "node build/js/index.js",
    "eslint": "./node_modules/standard/bin/cmd.js 'src/**/*.js'",
    "dist": "./node_modules/.bin/webpack -d -w",
    "wds": "./node_modules/.bin/webpack-dev-server --content-base dist --hot --inline",
    "compile": "npm run build && npm run dist",
    "watch": "nodemon --ignore build/ --ignore dist/ --exec 'npm run compile'",
    "e2e-test": "./ete.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.11",
    "react-hot-loader": "^1.3.1",
    "selenium-server": "^3.0.1",
    "standard": "^8.6.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "material-ui": "^0.16.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "reflux": "^5.0.3",
    "superagent": "^3.1.0"
  }
}
