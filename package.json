{
  "name": "roy_forums_ui",
  "version": "1.0.0",
  "description": "just a ui for the rails project",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p build/css && npm run build-sass && ./node_modules/babel-cli/bin/babel.js --plugins transform-react-jsx src --out-dir build",
    "build-sass": "node-sass src/sass/main.scss dist/main.css",
    "build-watch": "mkdir -p build && ./node_modules/babel-cli/bin/babel.js --plugins transform-react-jsx src -w --out-dir build",
    "test": "./node_modules/mocha/bin/mocha --require babel-core/register -R spec",
    "eslint": "./node_modules/standard/bin/cmd.js 'src/**/*.js'",
    "dist": "mkdir -p dist && ./node_modules/.bin/webpack -d",
    "dist-watch": "mkdir -p dist && ./node_modules/.bin/webpack -d -w",
    "wds": "./node_modules/.bin/webpack-dev-server --content-base dist --hot --inline --port 9000 --host 0.0.0.0",
    "wds-docker": "NODE_ENV=docker ./node_modules/.bin/webpack-dev-server --content-base dist --hot --inline --port 9000 --host 0.0.0.0",
    "serve": "npm run compile && npm run dist && nodemon build/js/server.js",
    "start": "npm run build && npm run dist && npm run serve",
    "compile": "tsc --outDir build",
    "watch": "nodemon --ignore build/ --ignore dist/ --exec 'npm run compile'",
    "ete": "./ete.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.0",
    "@types/reflux": "^0.4.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.11",
    "node-sass": "^4.3.0",
    "node-static": "^0.7.9",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^4.1.1",
    "selenium-download": "^2.0.9",
    "source-map-loader": "^0.2.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/material-ui": "^0.16.58",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/superagent": "^2.0.36",
    "express": "^4.15.2",
    "material-ui": "^0.16.4",
    "moment": "^2.17.1",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "reflux": "^5.0.3",
    "superagent": "^3.1.0"
  }
}
